{
    "manifest_version": 3,
    "name": "Elitmus",
    "description": "Exam proctor",
    "version": "1.0.0",
    "action": {
        "default_title": "Exam Eye",
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo192.png",
            "128": "logo192.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab","scripting","downloads","tabs","videoCapture", "audioCapture","fullscreen","storage","notifications","alerts"]
}
